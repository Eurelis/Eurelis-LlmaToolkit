{
    "project": "llamaindex",
    "dataset": [
        {
            "id": "sitemap",
            "reader": {
                "provider": "AdvancedSitemapReader",
                "sitemap_url": "https://www.eurelis.com/sitemap.xml",
                "html_to_text": true,
                "limit": 1,
                "embed_pdf": true,
                "user_agent": "EurelisLLMATK/0.1",
                "url_include_filters": [
                    "^https:\\/\\/www\\.eurelis\\.com\\/blog.*"
                ],
                "parser_remove": [
                    "script",
                    {
                        "name": "header",
                        "attrs": {
                            "id": "masthead"
                        }
                    },
                    {
                        "name": "div",
                        "attrs": {
                            "class": "uagb-button__link"
                        }
                    },
                    "noscript",
                    "footer",
                    "svg",
                    {
                        "name": "nav",
                        "attrs": {
                            "class": "breadcrumb"
                        }
                    },
                    "form",
                    "button"
                ]
            },
            "transformations": [
                {
                    "provider": "SentenceSplitter",
                    "chunk_size": 768,
                    "chunk_overlap": 56
                }
            ],
            "acronyms": {
                "provider": "JSONFileAcronymTransformer",
                "path": "../etc/config_samples/acronyms.json"
            }
        },
        {
            "id": "url",
            "reader": {
                "provider": "SimpleWebPageReader",
                "urls": [
                    "https://www.eurelis.com/blog/potentiel-amplifie-par-ia-opportunites-du-couplage-entre-les-llm-et-les-bdd-vectorielles/"
                ],
                "html_to_text": true,
                "limit": 1
            },
            "transformations": [
                {
                    "provider": "SentenceSplitter",
                    "chunk_size": 768,
                    "chunk_overlap": 56
                }
            ],
            "acronyms": {
                "provider": "JSONFileAcronymTransformer",
                "path": "../etc/config_samples/acronyms.json"
            }
        },
        {
            "id": "txt",
            "reader": {
                "provider": "TXTFileReader",
                "base_dir": "../etc/data_sample/",
                "glob": "*.txt"
            },
            "transformations": [
                {
                    "provider": "SentenceSplitter",
                    "chunk_size": 768,
                    "chunk_overlap": 56
                }
            ],
            "acronyms": {
                "provider": "JSONFileAcronymTransformer",
                "path": "../etc/config_samples/acronyms.json"
            }
        },
        {
            "id": "pdf",
            "reader": {
                "provider": "PDFFileReader",
                "base_dir": "../etc/data_sample/",
                "glob": "*.pdf"
            },
            "transformations": [
                {
                    "provider": "SentenceSplitter",
                    "chunk_size": 768,
                    "chunk_overlap": 56
                }
            ],
            "acronyms": {
                "provider": "JSONFileAcronymTransformer",
                "path": "../etc/config_samples/acronyms.json"
            }
        }
    ],
    "scraping_cache": {
        "provider": "FSCache",
        "base_dir": "../etc/no-commit/scraping_cache"
    },
    "embeddings": {
        "provider": "OpenAI",
        "model": "text-embedding-3-small",
        "openai_api_key": "${OPENAI_API_KEY}"
    },
    "vectorstore": {
        "provider": "MongoDB",
        "url": "${MONGO_CONNECTION_STRING}",
        "db_name": "${MONGO_DB_NAME}",
        "collection_name": "${MONGO_COLLECTION_NAME}",
        "vector_index_name": "vector_index"
    },
    "_vectorstore": {
        "provider": "Chroma",
        "mode": "persistent",
        "path": "../etc/chroma.db",
        "collection_name": "XXX"
    },
    "documentstore": {
        "provider": "MongoDB",
        "url": "${MONGO_CONNECTION_STRING}",
        "db_name": "${MONGO_DB_NAME}"
    }
}